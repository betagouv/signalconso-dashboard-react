  "type": "module",
  "engines": {
    "node": "22.13.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "dev": "vite",
    "devdemo": "VITE_APP_API_BASE_URL=https://demo-signalement-api.cleverapps.io VITE_APP_COMPANY_API_BASE_URL=https://app-8d9df60d-88b2-4930-9f75-1068520ae39f.cleverapps.io vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint . --max-warnings=0",
    "format": "npx prettier --write .",
    "checkformat": "npx prettier --check .",
    "checktypes": "tsc --noEmit",
    "checkall": "yarn checktypes && yarn checkformat && yarn lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
